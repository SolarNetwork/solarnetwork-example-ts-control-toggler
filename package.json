{
  "name": "solarnetwork-example-typescript-control-toggler",
  "private": true,
  "version": "1.1.1",
  "homepage": "https://go.solarnetwork.net/dev/example/typescript-control-toggler/",
  "author": {
    "name": "Matt Magoffin",
    "email": "matt@solarnetwork.net"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SolarNetwork/solarnetwork-example-ts-control-toggler.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "ava": {
    "files": [
      "src/test/ts/**/*"
    ],
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      },
      "compile": "tsc"
    }
  },
  "c8": {
    "exclude": [
      "src/test/ts/**"
    ],
    "exclude-after-remap": true,
    "reporter": [
      "text",
      "html"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "d3-selection": "^3.0.0",
    "solarnetwork-api-core": "^3.4.0"
  },
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@eslint/js": "^9.35.0",
    "@types/bootstrap": "^5.2.10",
    "@types/d3-selection": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "npmversion": "^2.0.5",
    "sass": "^1.92.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  },
  "overrides": {
    "json5": "2.2.3",
    "lodash": "4.17.21",
    "minimist": "1.2.8",
    "postcss": "8.4.31",
    "semver": "7.5.4"
  }
}
